        <ImageView android:id="@id/minute_unit2" android:layout_width="@dimen/stopwatch_digit_width" android:layout_height="@dimen/stopwatch_digit_height" android:layout_marginTop="@dimen/stopwatch_digital_marginTop" android:layout_marginRight="@dimen/stopwatch_minute_unit_marginRight" />
        <ImageView android:id="@id/digit_colon2" android:layout_width="@dimen/stopwatch_colon_width" android:layout_height="@dimen/stopwatch_colon_height" android:layout_marginTop="@dimen/stopwatch_digital_marginTop" android:layout_marginRight="@dimen/stopwatch_digit_colon_marginRight" />
        <ImageView android:id="@id/second_ten2" android:layout_width="@dimen/stopwatch_digit_width" android:layout_height="@dimen/stopwatch_digit_height" android:layout_marginTop="@dimen/stopwatch_digital_marginTop" android:layout_marginRight="@dimen/stopwatch_second_ten_marginRight" />
        <ImageView android:id="@id/second_unit2" android:layout_width="@dimen/stopwatch_digit_width" android:layout_height="@dimen/stopwatch_digit_height" android:layout_marginTop="@dimen/stopwatch_digital_marginTop" android:layout_marginRight="@dimen/stopwatch_second_unit_marginRight" />
