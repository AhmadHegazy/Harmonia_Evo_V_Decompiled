        <ImageView android:id="@id/digit_colon" android:layout_width="@dimen/stopwatch_colon_width" android:layout_height="@dimen/stopwatch_colon_height" android:layout_marginTop="@dimen/stopwatch_digital_marginTop" android:layout_marginRight="@dimen/stopwatch_digit_colon_marginRight" />
