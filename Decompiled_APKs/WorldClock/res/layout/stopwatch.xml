        <Button android:ellipsize="marquee" android:id="@id/lapbtn" android:layout_width="@dimen/stopwatch_lapbtn_width" android:layout_height="@dimen/stopwatch_lapbtn_height" android:layout_marginLeft="@dimen/stopwatch_lapbtn_marginLeft" android:text="@string/lap" android:singleLine="true" android:layout_toRightOf="@id/btn1" style="@com.htc:style/list_primary_l" />
        <Button android:ellipsize="marquee" android:id="@id/btn2" android:layout_width="@dimen/stopwatch_btn2_width" android:layout_height="@dimen/stopwatch_btn2_height" android:layout_marginLeft="@dimen/stopwatch_btn2_marginLeft" android:text="@string/reset" android:singleLine="true" android:layout_toRightOf="@id/lapbtn" style="@com.htc:style/list_primary_l" />
