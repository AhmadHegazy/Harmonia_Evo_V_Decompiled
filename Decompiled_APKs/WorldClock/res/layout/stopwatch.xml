    <com.htc.widget.HtcListView android:id="@id/htclist" android:background="@color/listview_bg_color" android:fadingEdge="none" android:layout_width="@dimen/stopwatch_list_width" android:layout_height="@dimen/stopwatch_list_height" android:layout_marginLeft="@dimen/stopwatch_list_marginLeft" android:layout_marginTop="@dimen/stopwatch_list_marginTop" android:cacheColorHint="@*android:color/transparent" android:divider="@com.htc:drawable/common_list_divider" android:layout_weight="1.0" />
</RelativeLayout>