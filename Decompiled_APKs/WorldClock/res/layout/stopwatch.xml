        <ImageView android:id="@id/millisecond2" android:layout_width="@dimen/stopwatch_digit_width" android:layout_height="@dimen/stopwatch_digit_height" android:layout_marginTop="@dimen/stopwatch_digital_marginTop" />
    </LinearLayout>
    <TextView android:id="@id/lap_title" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/lap_title_marginLeft" android:layout_marginTop="@dimen/lap_title_marginTop" android:layout_marginRight="@dimen/lap_title_marginRight" android:text="@string/lap_title" android:layout_alignParentRight="@string/lap_title_alignParentRight" style="@com.htc:style/darklist_secondary_unread_m" />
